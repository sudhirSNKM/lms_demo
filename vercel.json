{
    "version": 2,
    "rewrites": [
        {
            "source": "/",
            "destination": "/frontend/index.html"
        },
        {
            "source": "/index.html",
            "destination": "/frontend/index.html"
        },
        {
            "source": "/dashboard.html",
            "destination": "/frontend/dashboard.html"
        },
        {
            "source": "/login.html",
            "destination": "/frontend/login.html"
        },
        {
            "source": "/css/:match*",
            "destination": "/frontend/css/:match*"
        },
        {
            "source": "/js/:match*",
            "destination": "/frontend/js/:match*"
        }
    ]
}